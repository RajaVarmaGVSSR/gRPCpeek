{
  "name": "grpc-test-server",
  "version": "1.0.0",
  "description": "Test gRPC server for gRPCpeek",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node test.js",
    "generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:. --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` test.proto",
    "generate-ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=`which protoc-gen-ts` --ts_out=. --js_out=import_style=commonjs,binary:. --grpc_out=. test.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "@grpc/proto-loader": "^0.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "grpc-tools": "^1.12.0",
    "grpc_tools_node_protoc_ts": "^5.3.0"
  }
}